(

cd meta
mkdir repo
for i in *; do 
    (
        x gh cl x-cmd-arxiv/$i repo/$i; 
        command cp $i/* repo/$i/ || {
            x:error "fail at cp => $i"
            return 1
        }
        cd repo/$i
        git comit -m ... || {
            x:error "fail at commit => $i"
            return 1
        }
        git push origin main || {
            x:error "fail at push => $i"
            return 1
        }
    )
done

)
